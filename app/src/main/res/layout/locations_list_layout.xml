<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
     android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Location :"
                android:paddingTop="4dp"
                android:layout_marginTop="8dp"
                android:paddingLeft="8dp"
                android:textStyle="bold"
                android:textColor="#000000">

            </TextView>
    <TextView
        android:id="@+id/textVieLocation"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:paddingLeft="16dp"
        android:paddingTop="4dp"
        android:layout_marginTop="8dp"/>
        </LinearLayout>

<!--    <TextView-->
<!--    android:id="@+id/textViewLatLng"-->
<!--    android:layout_width="match_parent"-->
<!--    android:layout_height="wrap_content"-->
<!--    android:text="TextView"-->
<!--        android:paddingLeft="16dp"/>-->
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Time Avlbl:"
                android:paddingTop="4dp"

                android:paddingLeft="8dp"
                android:textStyle="bold">

            </TextView>
    <TextView
        android:id="@+id/textViewTimeAvl"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:paddingTop="4dp"
        android:paddingLeft="16dp"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Time of Registration"
                android:paddingTop="4dp"
                android:paddingLeft="8dp"
                android:textStyle="bold">

            </TextView>

    <TextView
        android:id="@+id/textViewTimeOfAccess"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:paddingTop="4dp"
        android:paddingLeft="16dp"/>
        </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:paddingBottom="8dp">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=""
                android:paddingTop="4dp"

                android:paddingLeft="8dp"
                android:textStyle="bold">
                
            </TextView>

        <TextView
            android:id="@+id/textViewClickToViewLoc"
